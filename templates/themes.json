  "slideLayouts": {
    "title": {
      "template": "center-content",
      "supports": ["emoji", "heading", "subheading"]
    },
    "bullets": {
      "template": "content",
      "supports": ["heading", "items"]
    },
    "grid-2x2": {
      "template": "grid-2x2",
      "supports": ["heading", "cards"],
      "cardCount": 4
    },
    "grid-3col": {
      "template": "grid-3col",
      "supports": ["heading", "cards"],
      "cardCount": 3
    },
    "two-column": {
      "template": "split-layout",
      "supports": ["heading", "left", "right"]
    },
    "quote": {
      "template": "quote-slide",
      "supports": ["quote", "author"]
    },
    "numbered": {
      "template": "numbered-sections",
      "supports": ["heading", "steps"]
    },
    "conclusion": {
      "template": "center-content",
      "supports": ["heading", "message", "submessage"]
    }
  }
}
